{
  "version": 2,
  "functions": {
    "api/tasks.ts": {
      "runtime": "@vercel/node@3.2.22"
    }
  },
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/tasks",
      "dest": "/api/tasks"
    },
    {
      "src": "/(.*)",
      "dest": "/client/dist/$1"
    }
  ]
}