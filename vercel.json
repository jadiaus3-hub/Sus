{
  "version": 2,
  "functions": {
    "api/tasks.ts": {
      "runtime": "@vercel/node@3.2.22"
    }
  },
  "buildCommand": "cd client && npm run build",
  "outputDirectory": "client/dist",
  "rewrites": [
    {
      "source": "/api/tasks",
      "destination": "/api/tasks"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}